    "@11ty/eleventy": "0.5.4",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.1",
    "gulp-uglify": "^3.0.1",
    "html-minifier": "^3.5.20",
    "netlify-lambda": "^1.1.0",
    "twitter": "^1.7.1"
